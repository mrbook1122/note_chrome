{
    "name": "note",
    "manifest_version": 2,
    "version": "1.0",

    "browser_action": {

    },

    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },

    "permissions": [
        "tabs",
        "activeTab",
        "*://*/*",
        "tabCapture",
        "desktopCapture"
    ]
}